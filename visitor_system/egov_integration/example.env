# Пример настроек для интеграции с egov.kz
# Скопируйте эти переменные в ваш основной .env файл

# API ключ egov.kz (получите на data.egov.kz)
EGOV_API_KEY=63092dca612f4a79b9c019c06ea21b3a

# Базовый URL API (production)
EGOV_BASE_URL=https://data.egov.kz/api/v4

# Тестовый URL (если есть sandbox)
# EGOV_BASE_URL=https://test-data.egov.kz/api/v4

# Таймауты и лимиты
EGOV_TIMEOUT=30
EGOV_MAX_RETRIES=3
EGOV_RATE_LIMIT_PER_MINUTE=60

# Кеширование
EGOV_CACHE_TTL_SECONDS=300

# Логирование (debug/info/warning/error)
EGOV_LOG_LEVEL=info

# Включить/выключить интеграцию
EGOV_ENABLED=true
